<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.myshop">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:name=".app.MyApp"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme"
        android:usesCleartextTraffic="true">
        <activity android:name=".ui.LauncherActivity"></activity>
        <activity android:name=".model.shop.ShopAddressesAddActivity" />
        <activity android:name=".ui.shop.ShopAddressesActivity" />
        <activity android:name=".ui.RegistActivity" />
        <activity android:name=".ui.me.UserDetailActivity" />
        <activity android:name=".ui.shop.OrderActivity" />
        <activity android:name=".ui.me.MyCollectionActivity" />
        <activity android:name=".ui.topic.TopicActivity" />
        <activity android:name=".ui.LoginActivity" />
        <activity android:name=".ui.home.BigImageActivity" />
        <activity android:name=".ui.home.HomeItemDetailsActivity" />
        <activity android:name=".ui.home.HomeNewGoodTitleActivity" />
        <activity android:name=".ui.home.HomeBrandTitleImgActivity" />
        <activity android:name=".ui.home.HomeBrandTitleActivity" />
        <activity android:name=".ui.home.HomeItemActivity" />
        <activity android:name=".ui.SplaceActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".ui.MainActivity" />
    </application>

</manifest>